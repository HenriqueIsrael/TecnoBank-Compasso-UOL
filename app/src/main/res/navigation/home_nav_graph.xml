<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.tecnobank.home.fragments.HomeFragment"
        android:label="inicio"
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_homeFragment_to_pixQrCodeActivity"
            app:destination="@id/pixQrCodeActivity" />
        <action
            android:id="@+id/action_homeFragment_to_accountDependencyActivity"
            app:destination="@id/accountDependencyActivity" />
        <action
            android:id="@+id/action_homeFragment_to_notificationActivity"
            app:destination="@id/notificationActivity" />
        <action
            android:id="@+id/action_homeFragment_to_qrCodeSafeActivity"
            app:destination="@id/qrCodeSafeActivity" />
        <action
            android:id="@+id/action_homeFragment_to_profileActivity"
            app:destination="@id/profileActivity" />
        <action
            android:id="@+id/action_homeFragment_to_cardsFragment"
            app:destination="@id/cardsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_payQrCodeActivity"
            app:destination="@id/payQrCodeActivity" />
    </fragment>
    <fragment
        android:id="@+id/extractFragment"
        android:name="com.example.tecnobank.extract.fragments.ExtractFragment"
        android:label="extrato"
        tools:layout="@layout/extract_fragment">
        <action
            android:id="@+id/action_extratoFragment_to_filterActivity"
            app:destination="@id/filterActivity" />
    </fragment>
    <fragment
        android:id="@+id/cardsFragment"
        android:name="com.example.tecnobank.cards.fragments.CardsFragment"
        android:label="cartoes"
        tools:layout="@layout/cards_fragment" >
        <action
            android:id="@+id/action_cardsFragment_to_requestCardActivity"
            app:destination="@id/requestCardActivity" />
        <action
            android:id="@+id/action_cardsFragment_to_prepaidCardActivity"
            app:destination="@id/prepaidCardActivity" />
        <action
            android:id="@+id/action_cardsFragment_to_tecnobankCardsActivity"
            app:destination="@id/tecnobankCardsActivity" />
    </fragment>
    <fragment
        android:id="@+id/salesFragment"
        android:name="com.example.tecnobank.sales.fragments.SalesFragment"
        android:label="vendas"
        tools:layout="@layout/sales_fragment" />
    <activity
        android:id="@+id/filterActivity"
        android:name="com.example.tecnobank.extract.FilterActivity"
        android:label="complement_activity"
        tools:layout="@layout/filter_activity" />
    <activity
        android:id="@+id/pixQrCodeActivity"
        android:name="com.example.tecnobank.home.PixQrCodeActivity"
        android:label="PixQrCodeActivity"
        tools:layout="@layout/pix_qr_code_fragment" />
    <activity
        android:id="@+id/accountDependencyActivity"
        android:name="com.example.tecnobank.home.AccountDependencyActivity"
        android:label="account_dependency_activity"
        tools:layout="@layout/account_dependency_activity" />
    <activity
        android:id="@+id/notificationActivity"
        android:name="com.example.tecnobank.home.NotificationActivity"
        android:label="notification_activity"
        tools:layout="@layout/notification_activity" />
    <activity
        android:id="@+id/qrCodeSafeActivity"
        android:name="com.example.tecnobank.home.QrCodeSafeActivity"
        android:label="qr_code_safe_activity"
        tools:layout="@layout/qr_code_safe_activity" />
    <activity
        android:id="@+id/profileActivity"
        android:name="com.example.tecnobank.home.ProfileActivity"
        android:label="profile_activity"
        tools:layout="@layout/profile_activity" />
    <activity
        android:id="@+id/requestCardActivity"
        android:name="com.example.tecnobank.cards.RequestCardActivity"
        android:label="request_card_activity"
        tools:layout="@layout/request_card_activity" />
    <activity
        android:id="@+id/prepaidCardActivity"
        android:name="com.example.tecnobank.cards.PrepaidCardActivity"
        android:label="prepaid_card_activity"
        tools:layout="@layout/prepaid_card_activity" />
    <activity
        android:id="@+id/tecnobankCardsActivity"
        android:name="com.example.tecnobank.cards.TecnobankCardsActivity"
        android:label="tecnobank_cards_activity"
        tools:layout="@layout/tecnobank_cards_activity" />
    <activity
        android:id="@+id/payQrCodeActivity"
        android:name="com.example.tecnobank.home.PayQrCodeActivity"
        android:label="pay_qr_code_activity"
        tools:layout="@layout/pay_qr_code_activity" />
</navigation>